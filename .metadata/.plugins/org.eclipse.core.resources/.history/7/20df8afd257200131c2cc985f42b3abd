package ui;

public class Test {
	public static void main(String[] args)
	{
		String solved = "1234";
		int length=solved.length();
		String unsolved="";
		int counter=0;
		for(int i=0; i<length;i++)
		{
			if(counter==solved.charAt(i))
			{
				counter++;
			}
			else
			{
				unsolved=unsolved+counter;
			}
		}
		System.out.println(unsolved);
	}
}
